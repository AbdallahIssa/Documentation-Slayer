{
  "name": "runnable-info-extractor",
  "displayName": "Documentation Slayer",
  "description": "Automate the process of unit design Documentation for Classic AUTOSAR Projects and exports them to a structured Excel spreadsheet.",
  "publisher": "AbdallahIssa",
  "version": "3.0.0",
  "icon": "Vehiclevo_Logo.jpg",
  "engines": {
    "vscode": "^1.100.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdallahIssa/Documentation-Slayer.git"
  },
  "bugs": {
    "url": "https://github.com/AbdallahIssa/Documentation-Slayer/issues"
  },
  "homepage": "https://github.com/AbdallahIssa/Documentation-Slayer",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Run-Documentation-Slayer.Run-Documentation-Slayer",
        "title": "Run Documentation Slayer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/exceljs": "^0.5.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "exceljs": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  }
}
